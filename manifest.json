{
  "action": {
    "browser_style": true,
    "default_title": "List cookies in the active tab",
    "default_popup": "cookies.html",
    "default_icon": {
      "19": "icons/default19.png",
      "38": "icons/default38.png"
    }
  },
  "description": "List cookies in the active tab.",
  "icons": {
    "48": "icons/cookie.png",
    "96": "icons/cookie@2x.png"
  },
  "homepage_url": "https://rindevlabs.vercel.app",
  "manifest_version": 3,
  "name": "Transfer Cookies",
  "author": "Rindev",
  "version": "1.0",
  "permissions": ["cookies","tabs", "activeTab"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
